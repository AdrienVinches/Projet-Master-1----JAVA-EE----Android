<div id="container">
<div id="left">
<ui:repeat
	value="#{menuManager.sidebarLeft.listMenuItem}" var="menuItem">
	<div class="bloc-mini boxMenu">
	<h2><h:outputLink value="#{webGlobal.url_host}#{menuItem.url}"
		rendered="#{menuItem.oneTypeLink == 'relative'}" class="titleBox">#{menuItem.name}</h:outputLink>
	<h:outputLink value="#{menuItem.url}"
		rendered="#{menuItem.oneTypeLink != 'relative'}" class="titleBox">#{menuItem.name}</h:outputLink>
	<a href="#" title="rÃ©duire" class="uiBttReduce collapse">Collapse</a></h2>
	<div class="bloc-in">
	<ul>
		<ui:repeat value="#{menuItem.listChildren}" var="children">
			<li style="list-style-image: url('#{webGlobal.url_resource}#{children.urlMenuIcon}');">
				<h:outputLink value="#{webGlobal.url_host}#{children.url}"
					rendered="#{children.oneTypeLink == 'relative' and children.url == webGlobal.url_current}"
					class="titleBox currentSubMenu selectMenuItem">#{children.name}
				</h:outputLink> 
				<h:outputLink value="#{webGlobal.url_host}#{children.url}"
					rendered="#{children.oneTypeLink == 'relative' and children.url != webGlobal.url_current}"
					class="titleBox">#{children.name}
				</h:outputLink> 
				<h:outputLink value="#{children.url}"
				rendered="#{children.oneTypeLink != 'relative'}" class="titleBox">#{children.name}</h:outputLink>
			</li>
		</ui:repeat>
	</ul>
	</div>
	</div>
</ui:repeat></div>
