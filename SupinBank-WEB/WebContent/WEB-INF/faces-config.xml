<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
 <converter>
  <converter-id>convertAccount</converter-id>
  <converter-class>com.supinbank.converter.ConvertAccount</converter-class>
 </converter>
 <converter>
  <converter-id>convertAccountTransfert</converter-id>
  <converter-class>com.supinbank.converter.ConvertAccountTransfert</converter-class>
 </converter>
 <managed-bean>
  <managed-bean-name>auth</managed-bean-name>
  <managed-bean-class>com.supinbank.auth.managedbeans.Auth</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>sessionUser</managed-bean-name>
  <managed-bean-class>com.supinbank.auth.managedbeans.SessionUser</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>listAccount</managed-bean-name>
  <managed-bean-class>com.supinbank.customer.account.ListAccount</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>addCustomer</managed-bean-name>
  <managed-bean-class>com.supinbank.advisor.customer.AddCustomer</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>addAccount</managed-bean-name>
  <managed-bean-class>com.supinbank.advisor.customer.AddAccount</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>listCustomer</managed-bean-name>
  <managed-bean-class>com.supinbank.advisor.customer.ListCustomer</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>customerAccount</managed-bean-name>
  <managed-bean-class>com.supinbank.advisor.customer.CustomerAccount</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>addAccountToCustomer</managed-bean-name>
  <managed-bean-class>com.supinbank.advisor.customer.AddAccountToCustomer</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>addMoneyToCustomer</managed-bean-name>
  <managed-bean-class>com.supinbank.advisor.customer.AddMoneyToCustomer</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>listOperation</managed-bean-name>
  <managed-bean-class>com.supinbank.customer.account.ListOperation</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>performTransfert</managed-bean-name>
  <managed-bean-class>com.supinbank.customer.account.PerformTransfert</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <navigation-rule>
  <display-name>welcome_page_customer</display-name>
  <from-view-id>*</from-view-id>
  <navigation-case>
   <from-outcome>welcome_page_customer</from-outcome>
   <to-view-id>/modules/customer/account/listAccount.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>welcome_page_Advisor</from-outcome>
   <to-view-id>/modules/advisor/customer/main.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>advisorAddCustomer</from-outcome>
   <to-view-id>/modules/advisor/customer/addCustomer.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>addAccount</from-outcome>
   <to-view-id>/modules/advisor/customer/addAccount.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>listCustomer</from-outcome>
   <to-view-id>/modules/advisor/customer/listCustomer.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>customerAccount</from-outcome>
   <to-view-id>/modules/advisor/customer/customerAccount.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>addAccountToCustomer</from-outcome>
   <to-view-id>/modules/advisor/customer/addAccountToCustomer.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>addMoneyToAccount</from-outcome>
   <to-view-id>/modules/advisor/customer/addMoneyToAccount.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>operation</from-outcome>
   <to-view-id>/modules/customer/account/operation.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>performTransfert</from-outcome>
   <to-view-id>/modules/customer/account/performTransfert.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>main</from-outcome>
   <to-view-id>/auth/main.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <application>
  <view-handler>com.sun.facelets.FaceletViewHandler</view-handler>
 </application>
</faces-config>
